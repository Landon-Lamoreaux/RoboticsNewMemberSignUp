<Window x:Class="RoboticsNewMembers.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:RoboticsNewMembers"
        mc:Ignorable="d"
        Title="New Member Sign Up"
        Icon="/roboticslogo.png" Height="450" Width="800">
    <StackPanel VerticalAlignment="Stretch" Height="auto" Orientation="Horizontal">
        <TextBox x:Name="MemberNameBox" Text="{Binding MemberName}" HorizontalAlignment="Left" VerticalAlignment="Top" Width="200"></TextBox>
    </StackPanel>
</Window>
